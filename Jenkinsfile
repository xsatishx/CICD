pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Build running'
            }
        } 

	stage('test') {
	   steps {
	       echo 'Testing'
	       }
	}
     
        stage('Deploy') {
	   steps {
	       echo 'Deployed'
	       }
	}

    }
}
